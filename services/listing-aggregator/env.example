# Real Estate Listing Aggregator Environment Variables
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=4000
ENVIRONMENT=development

# Database Configuration
DB_USERNAME=postgres
DB_PASSWORD=your_password_here
DB_HOST=localhost
DB_PORT=5432
DATABASE_URL=postgresql://localhost/listing_aggregator_development

# Real Estate API Keys
# Get your RapidAPI key from: https://rapidapi.com/
# Sign up at: https://rapidapi.com/ and subscribe to these APIs:
# - Realty Mole Property API
# - RentSpree API  
# - Zillow API
RAPIDAPI_KEY=your_rapidapi_key_here

# Alternative APIs (optional)
# Redfin API (requires separate registration)
REDFIN_API_KEY=your_redfin_api_key_here
# Realtor.com API (requires separate registration)
REALTOR_API_KEY=your_realtor_api_key_here

# Cache Configuration (for future use)
# REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info 